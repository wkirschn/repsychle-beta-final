                                    <script async>
                                        // Initialize and add the map

                                        var map;
                                        var service;
                                        var infowindow;

                                        let lat = {{recycling.objectLat}};
                                        let lng = {{recycling.objectLong}};

                                        function initMap() {
                                            // The location of Uluru
                                            const uluru = { lat, lng };
                                            // The map, centered at Uluru
                                            const map = new google.maps.Map(document.getElementById("map"), {
                                                zoom: 10,
                                                center: uluru,
                                            });

                                            var request = {
                                                location: uluru,
                                                radius: '500',
                                                query: 'recycle'
                                            };



                                            const image =
                                                    "https://i.imgur.com/5dG0UL4.png";
                                            // The marker, positioned at Uluru
                                            const marker = new google.maps.Marker({
                                                position: uluru,
                                                map: map,
                                                title: "Recycle Object",
                                                icon: image,
                                            });


                                            marker.setMap(map);


                                        }


                                    </script>


